    // ANSI Escape Sequences
    // Standard escape codes are prefixed with Escape:
    // Ctrl-Key: ^[
    // Octal: \033
    // Unicode: \u001b
    // Hexadecimal: \x1b
    // Decimal: 27

    "\u001b[2A": "SelectUp",                       // Shift-Up
    "\u001b[3A": "command-edit:AltlUp",            // Alt-Up
    "\u001b[4A": "command-edit:ShiftAltlUp",       // Shift-Alt-Up
    "\u001b[5A": "command-edit:CtrlUp",            // Ctrl-Up
    "\u001b[6A": "command-edit:ShiftCtrlUp",       // Shift-Ctrl-Up
    "\u001b[7A": "command-edit:CtrlAltUp",         // Ctrl-Alt-Up
    "\u001b[8A": "command-edit:ShiftCtrlAlt",      // Shift-Ctrl-Alt-Up

    "\u001b[2B": "SelectDown",                     // Shift-Down
    "\u001b[3B": "command-edit:AltDown",           // Alt-Down
    "\u001b[4B": "command-edit:ShiftAltDown",      // Shift-Alt-Down
    "\u001b[5B": "command-edit:CtrlDown",          // Ctrl-Down
    "\u001b[6B": "command-edit:ShiftCtrlDown",     // Shift-Ctrl-Down
    "\u001b[7B": "command-edit:CtrlAltDown",       // Ctrl-Alt-Down
    "\u001b[8B": "command-edit:ShiftCtrlAltDown",  // Shift-Ctrl-Alt-Down

    "\u001b[2C": "SelectRight",                    // Shift-Right
    "\u001b[3C": "command-edit:AltRight",          // Alt-Right
    "\u001b[4C": "command-edit:ShiftAltRight",     // Shift-Alt-Right
    "\u001b[5C": "command-edit:CtrlRight",         // Ctrl-Right
    "\u001b[6C": "command-edit:ShiftCtrlRight",    // Shift-Ctrl-Right
    "\u001b[7C": "command-edit:CtrlAltRight",      // Ctrl-Alt-Right
    "\u001b[8C": "command-edit:ShiftCtrlAltRight", // Shift-Ctrl-Alt-Right

    "\u001b[2D": "SelectLeft",                     // Shift-Left
    "\u001b[3D": "command-edit:AltLeft",           // Alt-Left
    "\u001b[4D": "command-edit:ShiftAltLeft",      // Shift-Alt-Left
    "\u001b[5D": "command-edit:CtrlLeft",          // Ctrl-Left
    "\u001b[6D": "command-edit:ShiftCtrlLeft",     // Shift-Ctrl-Left
    "\u001b[7D": "command-edit:CtrlAltLeft",       // Ctrl-Alt-Left
    "\u001b[8D": "command-edit:ShiftCtrlAltLeft",  // Shift-Ctrl-Alt-Left

    "Shift-Delete": "command-edit:ShiftDelete",    // Shift-Delete
    
    "\u001b[2H": "SelectToStartOfLine",            // Shift-Home
