    "\u001b[6H": "command-edit:ShiftCtrlHome",     // Shift-Ctrl-Home
    "\u001b[7H": "command-edit:CtrlAltHome",       // Ctrl-Alt-Home
    "\u001b[8H": "command-edit:ShiftCtrlAltHome",  // Shift-Ctrl-Alt-Home
                                                  
    "\u001b[2F": "SelectToEndOfLine",              // Shift-End
    "\u001b[3F": "command-edit:AltEnd",            // Alt-End
    "\u001b[4F": "command-edit:ShiftAltEnd",       // Shift-Alt-End
    "\u001b[5F": "command-edit:CtrlEnd",           // Ctrl-End
    "\u001b[6F": "command-edit:ShiftCtrlEnd",      // Shift-Ctrl-End
    "\u001b[7F": "command-edit:CtrlAltEnd",        // Ctrl-Alt-End
